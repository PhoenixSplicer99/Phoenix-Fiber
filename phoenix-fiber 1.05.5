                splice: {
                    fondid: { required: true, unique: true },
                    fda: { required: false },
                    fsa: { required: false },
                    placement: { required: false },
                    spec: { required: false },
                    ap_type: { required: false },
                    wkt_geom: { required: false, type: 'geometry' }
                },
                cabinet: {
                    cabinet_id: { required: true, unique: true },
                    type: { required: false },
