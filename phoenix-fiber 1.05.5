                document.getElementById('gemStatus').classList.add('show');
                document.getElementById('gemDetails').textContent = `Light source: ${lightSourceCable}`;
            }
            
            // Build cable map
            cableMap = mapCablesToPorts(cableData);
            
            // Process connections
