            saveHistoryState();
            
            // Reset processing state
            parsingErrors = [];
            processedConnections = [];
            cableMap = {};
            deadFiberMap = {};
            
            // Identify light source cable
            lightSourceCable = identifyLightSourceCable(cableData);
            
